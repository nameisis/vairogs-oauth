{
    "name": "vairogs/vairogs-oauth",
    "description": "Real Latvian CMS Utils: OAuth / OpenID / OpenID Connect library",
    "license": "MIT",
    "type": "symfony-bundle",
    "keywords": [
        "vairogs",
        "oauth",
        "openid",
        "openidconnect",
        "symfony"
    ],
    "authors": [
        {
            "name": "k0d3r1s",
            "homepage": "https://github.com/k0d3r1s",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "ext-json": "*",
        "symfony/dependency-injection": "*",
        "symfony/http-kernel": "*",
        "symfony/config": "*",
        "symfony/routing": "*",
        "symfony/http-foundation": "*",
        "vairogs/vairogs-core": "dev-master",
        "vairogs/vairogs-specification": "dev-master",
        "symfony/framework-bundle": "*",
        "abraham/twitteroauth": "*",
        "psr/http-message": "^1",
        "league/oauth2-client": "*",
        "league/oauth1-client": "*",
        "lcobucci/jwt": "*",
        "guzzlehttp/guzzle": "~6.0"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Vairogs\\Utils\\Oauth\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vairogs\\Utils\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "symfony": {
            "require": "4.1.*"
        }
    }
}
